{
  "name": "@typetensor/backend-wasm",
  "version": "0.2.1",
  "description": "WebAssembly backend for TypedTensor",
  "license": "MIT",
  "author": "Thomas Santerre <thomas@santerre.xyz>",
  "homepage": "https://github.com/typetensor/typetensor/tree/main/packages/backend-wasm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typetensor/typetensor.git",
    "directory": "packages/backend-wasm"
  },
  "bugs": {
    "url": "https://github.com/typetensor/typetensor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "typetensor-dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "bun": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "typetensor-dev": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "bun": "./dist/*.js",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "pkg",
    "README.md"
  ],
  "sideEffects": false,
  "dependencies": {
    "@typetensor/core": "workspace:*"
  },
  "devDependencies": {
    "@typetensor/test-utils": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.9.0"
  },
  "scripts": {
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir pkg",
    "build:ts": "tsup",
    "build": "bun run build:wasm && bun run build:ts",
    "build:watch": "tsup --watch",
    "clean:wasm": "rm -rf pkg",
    "clean:ts": "rm -rf dist",
    "clean": "bun run clean:wasm && bun run clean:ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts,.tsx",
    "test:wasm": "cd wasm && wasm-pack test --node",
    "test:ts": "bun test",
    "test": "bun run test:wasm && bun run test:ts",
    "test:watch": "bun test --watch --no-clear-screen",
    "prepublishOnly": "bun run clean && bun run build"
  }
}
