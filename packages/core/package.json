{
  "name": "@typetensor/core",
  "version": "0.2.0",
  "description": "Core tensor operations for TypedTensor",
  "license": "MIT",
  "author": "Thomas Santerre <thomas@santerre.xyz>",
  "homepage": "https://github.com/typetensor/typetensor/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typetensor/typetensor.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/typetensor/typetensor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "typetensor-dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "bun": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./*": {
      "typetensor-dev": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "bun": "./dist/*.js",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "typescript": ">=5.9.0"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "bun test",
    "test:watch": "bun test --watch --no-clear-screen",
    "prepublishOnly": "bun run clean && bun run build"
  }
}
